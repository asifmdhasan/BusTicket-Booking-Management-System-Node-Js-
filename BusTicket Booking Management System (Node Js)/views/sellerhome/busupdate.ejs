                    </nav>
                <div>
                	<br><br><br>
                	<center>
                			<h3>Update Businfo</h3>
		
<form method="post">
				<center>
				<table width="400px">
						<tr>
							<th>Bus Id :</th>
							<td><input type="text"placeholder="Name" name="busid"value="<%=busid%>" disabled></td>
						</tr>
						<tr>
							<th>Bus Name :</th>
							<td><input type="text"placeholder="Name" name="busname"value="<%=busname%>"></td>
						</tr>
	
						<tr>
							<th>Time :</th>
							<td><input type="text"placeholder="Time" name="time"value="<%=time%>"></td>
						</tr>
						
						<tr>
							<th>Price :</th>
							<td><input type="text"placeholder="Price" name="price"value="<%=price%>"></td>
						</tr>
	
						<tr>
							<th>Starting Point:</th>

							<td><input type="text"placeholder="Starting Point" name="start"value="<%=start%>"></td>
						</tr>	
	
						<tr>
							<th>Ending Point:</th>
							<td><input type="text"placeholder="Ending Point" name="end"value="<%=end%>"></td>
						</tr>
				</table>
				</center>
				<center>
					<br><input name="submit" type="submit" value="Submit"/>
				</center>
				</form>
			</td>
		</tr>
</center>
