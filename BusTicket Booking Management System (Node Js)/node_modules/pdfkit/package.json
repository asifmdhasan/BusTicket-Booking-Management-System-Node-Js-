    "type": "git",
    "url": "git+https://github.com/foliojs/pdfkit.git"
  },
  "bugs": {
    "url": "https://github.com/foliojs/pdfkit/issues"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "blob-stream": "^0.1.2",
    "brace": "^0.2.1",
    "brfs": "~2.0.1",
    "browserify": "^13.3.0",
    "codemirror": "~3.20.0",
    "eslint": "^5.3.0",
    "iconv-lite": "^0.4.13",
    "jade": "~1.1.5",
    "jest": "^23.4.2",
    "markdown": "~0.5.0",
    "prettier": "1.15.3",
    "rollup": "^0.65.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-cpy": "^1.0.0"
  },
  "dependencies": {
    "crypto-js": "^3.1.9-1",
